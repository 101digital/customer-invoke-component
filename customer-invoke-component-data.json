    "type": "git",
    "url": "https://github.com/101digital/customer-invoke-component.git"
  },
  "config": {
    "env": {
      "development": {
        "customerInvokeBaseUrl": "https://api-ud-ow-dev.101digital.io/membership-service/1.0.14"
      },
      "staging": {
        "customerInvokeBaseUrl": "https://api-ud-ow-dev.101digital.io/membership-service/1.0.14"
